
#include <stdio.h>
#include <stdlib.h>
#include <string.h>

struct humen {
    char name[50];
    char surname[50];
    char birth_year[10];
};

int bithday(const void *a, const void *b) {
    struct humen *n_a = (struct humen *) a;
    struct humen *n_b = (struct humen *) b;

    return strcmp(n_a->birth_year, n_b->birth_year);

}

int main() {
    struct humen people[4];
    int i;
    for (i = 0; i < 4; i++) {
        printf("Enter person %d:\n", i + 1);
        printf("name: ");
        scanf("%s", people[i].name);
        printf("last name: ");
        scanf("%s", people[i].surname);
        printf("birth_year: ");
        scanf("%s", people[i].birth_year);
    }
	
	qsort(people, 4, sizeof(struct humen), bithday);
    
	
	printf("\nUnsorted:\n");
    for (i = 0; i < 4; ++i) {
        printf("%s %s, birth_year: %s\n", people[i].name, people[i].surname, people[i].birth_year);
    }

    return 0;
}
