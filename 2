#include <stdio.h>
#include <math.h>
#include <stdbool.h>


double P(double a, double b, double c) {
  double sum = a + b + c; 
  return sum;
  }
double S(double a, double b, double c) {
    double s = (a + b + c) / 2;
    return sqrt(s * (s - a) * (s - b) * (s - c));
    return s;
    }
    
        return ;
    }
}
