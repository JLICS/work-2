#include <stdio.h>
#include <stdlib.h>
#include <string.h>

struct human {
    char name[50];
    char surname[50];
    char birth_year[10];
    char sex[10];
    char rost[10];
};




int main() {
    struct human people[4];
    int i;
    FILE *f2;

    f2 = fopen("file_lol.txt", "r");
    if (f2 == NULL) {
        printf("Error opening file\n");
        return 1;
    }

    for (i = 0; i < 4; ++i) {
        fscanf(f2, "%s %s %s %s %s", people[i].name, people[i].surname, people[i].birth_year, people[i].sex, people[i].rost);.
    }

    fclose(f2);

    int field;
    printf("Enter the field to sort by (1: name, 2: surname, 3: birth_year, 4: sex, 5: rost): \n");
    scanf("%d", &field);
	
	if (field == 1){
	int compare_by_name(const void *a, const void *b) {
    struct human *ha = (struct human *)a;
    struct human *hb = (struct human *)b;
    return strcmp(ha->name, hb->name);
	}
	}
	if (field == 2){
	int compare_by_surname(const void *a, const void *b) {
    struct human *ha = (struct human *)a;
    struct human *hb = (struct human *)b;
    return strcmp(ha->surname, hb->surname);
	}
	}
	if (field == 3){
	int compare_by_birth_year(const void *a, const void *b) {
    struct human *ha = (struct human *)a;
    struct human *hb = (struct human *)b;
    return strcmp(ha->birth_year, hb->birth_year);
	}
	}
	if (field == 4){
	int compare_by_sex(const void *a, const void *b) {
    struct human *ha = (struct human *)a;
    struct human *hb = (struct human *)b;
    return strcmp(ha->sex, hb->sex);
	}
	}
	if (field == 5){
	int compare_by_rost(const void *a, const void *b) {
    struct human *ha = (struct human *)a;
    struct human *hb = (struct human *)b;
    return strcmp(ha->rost, hb->rost);
	}
	}
    qsort(people, 4, sizeof(struct human), compare_func);

    for (i = 0; i < 4; i++) {
        printf("%s %s %s %s %s\n", people[i].name, people[i].surname, people[i].birth_year, people[i].sex, people[i].rost);
    }

    return 0;
}
