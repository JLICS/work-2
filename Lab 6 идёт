#include <stdio.h>

int main() {
    FILE *file_in, *file_out;
    char line[100];

    file_in = fopen("input.txt", "r");
    if (file_in == NULL) {
        printf("Error opening input file.\n");
        return 1;
    }
    file_out = fopen("output.txt", "w");
    if (file_out == NULL) {
        printf("Error opening output file.\n");
        return 1;
    fclose(file_in);
    fclose(file_out);

    return 0;
}
