#include <stdio.h>
#include <stdlib.h>
int main() {
    int m=5;
 double matrix[m][m];
    double One = 0;
    double Two = 0;
    int i,j;
    
 
    printf("vedite znachenia matrix luboe\n"); 
 for(i=0; i<m; i++)
 {
    for(j=0; j<m; j++) 
 {
 scanf("%lf", &matrix[i][j]);
        }
    }
    
    for (i= 0; i < m; i++) {
        One += matrix[i][i];
    }
    
    for ( i = 0; i < m; i++) {
        Two += matrix[i][2 - i];
    }
    
    printf("Summa 1 dioganal: %lf\n", One);
    printf("Summa 2 dioganal: %lf\n",Two);
    
 return 0; 
}
