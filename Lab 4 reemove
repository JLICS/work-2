#include <stdio.h>
#include <math.h>


double Perimeter(double a, double b, double c) {
    return a + b + c;
}


double calculate(double a, double b, double c) {
    double p = (a + b + c) / 2; 
    return sqrt(p * (p - a) * (p - b) * (p - c));
}

int main() {
    double a, b, c;
    printf ("")

    return 0;
}
