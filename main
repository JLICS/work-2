#include <stdio.h>
#include <math.h>
int main() {
    double a = 1.0;
    double b = 5.0;
    double c = 6.0;
    double d, x1, x2;
    
    
    d = b*b - 4*a*c;
    
    
    if (d >= 0) {
        
        x1 = (-b + sqrt(d)) / (2*a);
        x2 = (-b - sqrt(d)) / (2*a);
        
        
         printf("x1=%lf x2=%lf", x1, x2);
    } else {
        printf("Êîðíÿ íå ñóùåñòâóþò.\n");
    }
    
    return 0;
}
